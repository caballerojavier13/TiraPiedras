<div class="text-center signin-container">
<%= form_for(resource, as: resource_name, url: session_path(resource_name), class: 'form-signin') do |f| %>
  <h1 class="h3 mb-3 font-weight-normal">Iniciar Session</h1>

  <%= f.email_field :email, autofocus: true, class: 'form-control' %>
  <%= f.password_field :password, autocomplete: "off", class: 'form-control' %>
  <br/>
  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in", class: 'btn btn-lg btn-primary btn-block' %>
  </div>


    <%= render "devise/shared/links" %>
<% end %>
</div>